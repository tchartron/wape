{
  "name": "wape",
  "version": "0.1.2",
  "description": "Webpage application page editor",
  "main": "wape.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "rollup --config --watch --environment WATCH,BUILD:development",
    "build:dev": "rollup --config --environment BUILD:development",
    "build:prod": "rollup --config --environment BUILD:production && npm run minify-css",
    "minify-css": "postcss --replace dist/styles/*.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tchartron/wape.git"
  },
  "keywords": [
    "wape",
    "web",
    "page",
    "webpage",
    "builder",
    "editor",
    "html",
    "css",
    "layout"
  ],
  "author": "Thomas Chartron",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tchartron/wape/issues"
  },
  "homepage": "https://github.com/tchartron/wape#readme",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.3.4",
    "cssnano": "^4.1.10",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "rollup": "^2.39.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "mitt": "^2.1.0",
    "vue": "^2.6.12"
  }
}
